{
  "name": "lol-stats",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:riot-proxy": "nest build riot-proxy",
    "build:report": "nest build report",
    "build:user": "nest build user",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:riot-proxy": "nest start riot-proxy",
    "start:user": "nest start user",
    "start:discord-bot": "nest start discord-bot",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:riot-proxy": "nest start riot-proxy --watch",
    "start:dev:report": "nest start report --watch",
    "start:dev:user": "nest start user --watch",
    "start:debug": "nest start --debug --watch",
    "start:discord-bot:debug": "nest start discord-bot --debug --watch",
    "start:user:debug": "nest start user --debug --watch",
    "start:report:debug": "nest start report --debug --watch",
    "start:prod": "node dist/main",
    "start:riot-proxy:prod": "node dist/apps/riot-proxy/main.js",
    "start:user:prod": "node dist/apps/user/main.js",
    "start:report:prod": "node dist/apps/report/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/lol-stats/test/jest-e2e.json"
  },
  "dependencies": {
    "@discord-nestjs/common": "^3.4.2",
    "@discord-nestjs/core": "^3.6.0",
    "@fightmegg/riot-api": "^0.0.8",
    "@fightmegg/riot-rate-limiter": "^0.0.14",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^0.27.2",
    "cache-manager": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "discord.js": "^13.12.0",
    "luxon": "^3.0.4",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/luxon": "^3.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@sethtomy/riot-proxy-client(|/.*)$": "<rootDir>/libs/riot-proxy-client/src/$1",
      "^@sethtomy/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@sethtomy/user-client(|/.*)$": "<rootDir>/libs/user-client/src/$1",
      "^@sethtomy/http-client(|/.*)$": "<rootDir>/libs/http-client/src/$1",
      "^@sethtomy/report-client(|/.*)$": "<rootDir>/libs/report-client/src/$1"
    }
  }
}
